		<field xsi:type="DataSetFieldField">
			<dataPath>ГодВыпуска</dataPath>
			<field>ГодВыпуска</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	АвтомобилиСрезПоследних.Автомобиль КАК Автомобиль,
	АвтомобилиСрезПоследних.Значение КАК Значение
ПОМЕСТИТЬ втВладельцы
ИЗ
	РегистрСведений.Автомобили.СрезПоследних КАК АвтомобилиСрезПоследних
ГДЕ
	АвтомобилиСрезПоследних.ВидЗначения = &amp;Хозяин
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	МАКСИМУМ(ЗаказНаряд.Дата) КАК Дата,
	ЗаказНаряд.Автомобиль КАК Автомобиль
ПОМЕСТИТЬ втЗН
ИЗ
	Документ.ЗаказНаряд КАК ЗаказНаряд
ГДЕ
	ЗаказНаряд.Автомобиль &lt;&gt; &amp;Автомобиль

СГРУППИРОВАТЬ ПО
	ЗаказНаряд.Автомобиль
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаказНаряд.Ссылка КАК Ссылка,
	ЗаказНаряд.Дата КАК Дата,
	ЗаказНаряд.Автомобиль КАК Автомобиль
ПОМЕСТИТЬ внЗаказНаряды
ИЗ
	Документ.ЗаказНаряд КАК ЗаказНаряд
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РеализацияАвтомобилейАвтомобили.Автомобиль КАК Автомобиль,
	МИНИМУМ(РеализацияАвтомобилейАвтомобили.Ссылка.Дата) КАК Дата
ПОМЕСТИТЬ втРеализацияАвтомобиляДата
ИЗ
	Документ.РеализацияАвтомобилей.Автомобили КАК РеализацияАвтомобилейАвтомобили

СГРУППИРОВАТЬ ПО
	РеализацияАвтомобилейАвтомобили.Автомобиль
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	РеализацияАвтомобилейАвтомобили.Ссылка КАК Ссылка,
	РеализацияАвтомобилейАвтомобили.Автомобиль КАК Автомобиль
ПОМЕСТИТЬ втРеализацияАвтомобиля
ИЗ
	Документ.РеализацияАвтомобилей.Автомобили КАК РеализацияАвтомобилейАвтомобили
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Автомобили.Ссылка КАК Автомобиль,
	Автомобили.Модель КАК Модель,
	Автомобили.ГодВыпуска КАК ГодВыпуска,
	втВладельцы.Значение КАК Контрагент,
	втЗН.Дата КАК ДатаПоследнегоДокумента,
	втРеализацияАвтомобиляДата.Дата КАК ДатаПоследнегоДокументаРеализация
ПОМЕСТИТЬ втСводно
ИЗ
	Справочник.Автомобили КАК Автомобили
		ЛЕВОЕ СОЕДИНЕНИЕ втВладельцы КАК втВладельцы
		ПО Автомобили.Ссылка = втВладельцы.Автомобиль
		ЛЕВОЕ СОЕДИНЕНИЕ втЗН КАК втЗН
		ПО Автомобили.Ссылка = втЗН.Автомобиль
		ЛЕВОЕ СОЕДИНЕНИЕ втРеализацияАвтомобиляДата КАК втРеализацияАвтомобиляДата
		ПО Автомобили.Ссылка = втРеализацияАвтомобиляДата.Автомобиль
ГДЕ
	Автомобили.Модель.Родитель.Код = "ЦБ00000146"
	И Автомобили.ГодВыпуска МЕЖДУ &amp;ГодВыпускаНач И &amp;ГодВыпускаКон
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втСводно.Контрагент КАК Контрагент,
	втСводно.Контрагент.ОсновнойТелефон КАК Телефон,
	втСводно.Автомобиль КАК Автомобиль,
	втСводно.Модель КАК Модель,
	втСводно.ГодВыпуска КАК ГодВыпуска,
	втСводно.ДатаПоследнегоДокумента КАК ДатаПоследнегоДокумента,
	внЗаказНаряды.Ссылка КАК ЗаказНаряд,
	втРеализацияАвтомобиля.Ссылка КАК РеализацияАвтомобиля
ИЗ
	втСводно КАК втСводно
		ЛЕВОЕ СОЕДИНЕНИЕ внЗаказНаряды КАК внЗаказНаряды
		ПО втСводно.Автомобиль = внЗаказНаряды.Автомобиль
			И втСводно.ДатаПоследнегоДокумента = внЗаказНаряды.Дата
		ЛЕВОЕ СОЕДИНЕНИЕ втРеализацияАвтомобиля КАК втРеализацияАвтомобиля
		ПО втСводно.Автомобиль = втРеализацияАвтомобиля.Автомобиль
			И втСводно.ДатаПоследнегоДокументаРеализация = втРеализацияАвтомобиля.Ссылка.Дата</query>
	</dataSet>
