		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ДивизионыПодразделения.Ссылка КАК Дивизион,
	ЭМ_КумулятивнаяМаржа.Автомобиль.Модель КАК Модель,
	ЭМ_КумулятивнаяМаржа.РеализацияАвтомобиля КАК РеализацияАвтомобиля,
	СУММА(ВЫБОР
			КОГДА ЭМ_КумулятивнаяМаржа.Показатель = ЗНАЧЕНИЕ(Справочник.ЭМ_ПоказателиКумулятивнойМаржи.ФактМаржаТрейдИн)
				ТОГДА 0
			ИНАЧЕ ВЫБОР
					КОГДА ЭМ_КумулятивнаяМаржа.Показатель.ЭтоРасход
						ТОГДА -ЭМ_КумулятивнаяМаржа.Сумма
					ИНАЧЕ ЭМ_КумулятивнаяМаржа.Сумма
				КОНЕЦ - ВЫБОР
					КОГДА ЭМ_КумулятивнаяМаржа.Показатель.ЭтоРасход
						ТОГДА -ЭМ_КумулятивнаяМаржа.СуммаНДС
					ИНАЧЕ ЭМ_КумулятивнаяМаржа.СуммаНДС
				КОНЕЦ
		КОНЕЦ) КАК СуммаБезНДС
ПОМЕСТИТЬ вт
ИЗ
	РегистрНакопления.ЭМ_КумулятивнаяМаржа КАК ЭМ_КумулятивнаяМаржа
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Дивизионы.Подразделения КАК ДивизионыПодразделения
		ПО ЭМ_КумулятивнаяМаржа.РеализацияАвтомобиля.ПодразделениеКомпании = ДивизионыПодразделения.Подразделение
ГДЕ
	ЭМ_КумулятивнаяМаржа.Период МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ЭМ_КумулятивнаяМаржа.Регистратор.Проведен
	И ЭМ_КумулятивнаяМаржа.РеализацияАвтомобиля.Проведен

СГРУППИРОВАТЬ ПО
	ЭМ_КумулятивнаяМаржа.Автомобиль.Модель,
	ЭМ_КумулятивнаяМаржа.РеализацияАвтомобиля,
	ДивизионыПодразделения.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт.Дивизион КАК Дивизион,
	вт.Модель КАК Модель,
	СУММА(вт.СуммаБезНДС) КАК СуммаБезНДС
ПОМЕСТИТЬ вт_сумма
ИЗ
	вт КАК вт

СГРУППИРОВАТЬ ПО
	вт.Дивизион,
	вт.Модель
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт.Модель КАК Модель,
	вт.Дивизион КАК Дивизион,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ вт.РеализацияАвтомобиля) КАК КолСделок
ПОМЕСТИТЬ вт_КолСделок
ИЗ
	вт КАК вт

СГРУППИРОВАТЬ ПО
	вт.Модель,
	вт.Дивизион
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт_сумма.Дивизион КАК Дивизион,
	вт_сумма.Модель КАК Модель,
	ВЫРАЗИТЬ(вт_сумма.СуммаБезНДС / вт_КолСделок.КолСделок КАК ЧИСЛО(15, 0)) КАК Сумма
ПОМЕСТИТЬ вт_среднее
ИЗ
	вт_сумма КАК вт_сумма
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ вт_КолСделок КАК вт_КолСделок
		ПО вт_сумма.Дивизион = вт_КолСделок.Дивизион
			И вт_сумма.Модель = вт_КолСделок.Модель
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	МАКСИМУМ(вт_среднее.Сумма) КАК Сумма,
	вт_среднее.Модель КАК Модель
ПОМЕСТИТЬ вт_макс
ИЗ
	вт_среднее КАК вт_среднее

СГРУППИРОВАТЬ ПО
	вт_среднее.Модель
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	МИНИМУМ(вт_среднее.Сумма) КАК Сумма,
	вт_среднее.Модель КАК Модель
ПОМЕСТИТЬ вт_мин
ИЗ
	вт_среднее КАК вт_среднее

СГРУППИРОВАТЬ ПО
	вт_среднее.Модель
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт_сумма.Дивизион КАК Дивизион,
	вт_сумма.Модель КАК Модель,
	вт_макс.Сумма КАК СуммаМакс,
	вт_мин.Сумма КАК СуммаМин,
	вт_сумма.СуммаБезНДС КАК Сумма,
	вт_КолСделок.КолСделок КАК КолСделок
ИЗ
	вт_сумма КАК вт_сумма
		ЛЕВОЕ СОЕДИНЕНИЕ вт_макс КАК вт_макс
		ПО вт_сумма.Модель = вт_макс.Модель
		ЛЕВОЕ СОЕДИНЕНИЕ вт_мин КАК вт_мин
		ПО вт_сумма.Модель = вт_мин.Модель
		ЛЕВОЕ СОЕДИНЕНИЕ вт_КолСделок КАК вт_КолСделок
		ПО вт_сумма.Модель = вт_КолСделок.Модель
			И вт_сумма.Дивизион = вт_КолСделок.Дивизион</query>
