<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>ТекстЗапроса</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Артикул</dataPath>
			<field>Артикул</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Артикул</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<group>true</group>
			</useRestriction>
			<valueType>
				<v8:Type>xs:string</v8:Type>
				<v8:StringQualifiers>
					<v8:Length>25</v8:Length>
					<v8:AllowedLength>Variable</v8:AllowedLength>
				</v8:StringQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Валюта</dataPath>
			<field>Валюта</field>
			<title xsi:type="xs:string">Валюта</title>
			<useRestriction>
				<condition>true</condition>
			</useRestriction>
			<valueType>
				<v8:Type xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.Валюты</v8:Type>
			</valueType>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>МинимальнаяШирина</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">14</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Количество</dataPath>
			<field>Количество</field>
			<title xsi:type="xs:string">Количество</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<valueType>
				<v8:Type>xs:decimal</v8:Type>
				<v8:NumberQualifiers>
					<v8:Digits>15</v8:Digits>
					<v8:FractionDigits>3</v8:FractionDigits>
					<v8:AllowedSign>Nonnegative</v8:AllowedSign>
				</v8:NumberQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Контрагент</dataPath>
			<field>Контрагент</field>
			<title xsi:type="xs:string">Контрагент</title>
			<valueType>
				<v8:Type xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.Контрагенты</v8:Type>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Наименование</dataPath>
			<field>Наименование</field>
			<title xsi:type="xs:string">Наименование</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<valueType>
				<v8:Type>xs:string</v8:Type>
				<v8:StringQualifiers>
					<v8:Length>80</v8:Length>
					<v8:AllowedLength>Variable</v8:AllowedLength>
				</v8:StringQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НаименованиеИностранное</dataPath>
			<field>НаименованиеИностранное</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Наименование иностранное</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<valueType>
				<v8:Type>xs:string</v8:Type>
				<v8:StringQualifiers>
					<v8:Length>80</v8:Length>
					<v8:AllowedLength>Variable</v8:AllowedLength>
				</v8:StringQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Номенклатура</dataPath>
			<field>Номенклатура</field>
			<title xsi:type="xs:string">Номенклатура</title>
			<valueType>
				<v8:Type xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.Номенклатура</v8:Type>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ПрайсЛист</dataPath>
			<field>ПрайсЛист</field>
			<title xsi:type="xs:string">Прайс-лист</title>
			<valueType>
				<v8:Type xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.ПрайсЛистыКонтрагентов</v8:Type>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СрокПоставки</dataPath>
			<field>СрокПоставки</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Срок поставки</v8:content>
				</v8:item>
			</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<valueType>
				<v8:Type>xs:string</v8:Type>
				<v8:StringQualifiers>
					<v8:Length>0</v8:Length>
					<v8:AllowedLength>Variable</v8:AllowedLength>
				</v8:StringQualifiers>
			</valueType>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Цена</dataPath>
			<field>Цена</field>
			<title xsi:type="xs:string">Цена</title>
			<useRestriction>
				<condition>true</condition>
				<group>true</group>
				<order>true</order>
			</useRestriction>
			<valueType>
				<v8:Type>xs:decimal</v8:Type>
				<v8:NumberQualifiers>
					<v8:Digits>0</v8:Digits>
					<v8:FractionDigits>0</v8:FractionDigits>
					<v8:AllowedSign>Any</v8:AllowedSign>
				</v8:NumberQualifiers>
			</valueType>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>Формат</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">ЧЦ=15;ЧДЦ=2</dcscor:value>
				</dcscor:item>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>МинимальнаяШирина</dcscor:parameter>
					<dcscor:value xsi:type="xs:string">14</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Производитель</dataPath>
			<field>Производитель</field>
			<valueType>
				<v8:Type xmlns:d5p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d5p1:CatalogRef.Производители</v8:Type>
			</valueType>
		</field>
		<dataSource>ИсточникДанных</dataSource>
		<query>ВЫБРАТЬ
    КурсыВалютСрезПоследних.Валюта КАК Валюта,
    ВЫБОР
        КОГДА НЕ &amp;ПоВалютеОтчета
            ТОГДА 1
        КОГДА КурсыВалютСрезПоследних.Кратность = 0
            ТОГДА КурсыВалютСрезПоследних.Курс / &amp;КурсВалютыОтчета
        ИНАЧЕ КурсыВалютСрезПоследних.Курс / КурсыВалютСрезПоследних.Кратность / &amp;КурсВалютыОтчета
    КОНЕЦ КАК Коэффициент
ПОМЕСТИТЬ КурсыВалютСрезПоследних
ИЗ
    РегистрСведений.КурсыВалют.СрезПоследних(&amp;ДатаОкончания, ) КАК КурсыВалютСрезПоследних
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    Номенклатура.Ссылка КАК Ссылка,
    Номенклатура.Артикул КАК Артикул,
    Номенклатура.АртикулДляПоиска КАК АртикулДляПоиска,
    Номенклатура.Производитель КАК Производитель
ПОМЕСТИТЬ СписокНоменклатуры
ИЗ
    Справочник.Номенклатура КАК Номенклатура
{ГДЕ
    Номенклатура.Ссылка.* КАК Номенклатура,
    Номенклатура.Артикул КАК Артикул,
    Номенклатура.Производитель.* КАК Производитель}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
    СписокНоменклатуры.Ссылка КАК Номенклатура,
    СписокНоменклатуры.Производитель КАК Производитель,
    СписокНоменклатуры.Артикул КАК Артикул,
    ТаблицаРегистра.ПрайсЛист.Владелец КАК Контрагент,
    ТаблицаРегистра.ПрайсЛист КАК ПрайсЛист,
    ТаблицаРегистра.Наименование КАК Наименование,
    ТаблицаРегистра.НаименованиеИностранное КАК НаименованиеИностранное,
    ТаблицаРегистра.Цена * КурсыВалютСрезПоследних.Коэффициент КАК Цена,
    ВЫБОР
        КОГДА &amp;ПоВалютеОтчета
            ТОГДА &amp;ВалютаОтчета
        ИНАЧЕ ТаблицаРегистра.Валюта
    КОНЕЦ КАК Валюта,
    ТаблицаРегистра.Количество КАК Количество,
    ТаблицаРегистра.СрокПоставки КАК СрокПоставки,
    ТаблицаРегистра.ТегПозиции КАК ТегПозиции
ПОМЕСТИТЬ ВсеЦены
ИЗ
    РегистрСведений.ПрайсЛистыКонтрагентов КАК ТаблицаРегистра
        ВНУТРЕННЕЕ СОЕДИНЕНИЕ СписокНоменклатуры КАК СписокНоменклатуры
        ПО (СписокНоменклатуры.АртикулДляПоиска &lt;&gt; "")
            И (СписокНоменклатуры.АртикулДляПоиска = ТаблицаРегистра.АртикулДляПоиска)
            И (СписокНоменклатуры.Производитель = ТаблицаРегистра.Производитель)
        ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалютСрезПоследних КАК КурсыВалютСрезПоследних
        ПО ТаблицаРегистра.Валюта = КурсыВалютСрезПоследних.Валюта
{ГДЕ
    ТаблицаРегистра.ПрайсЛист.Владелец.* КАК Контрагент,
    ТаблицаРегистра.ПрайсЛист.* КАК ПрайсЛист}

ОБЪЕДИНИТЬ

ВЫБРАТЬ
    ВЫРАЗИТЬ(Рег.Значение КАК Справочник.Номенклатура).Ссылка,
    ВЫРАЗИТЬ(Рег.Значение КАК Справочник.Номенклатура).Производитель,
    ВЫРАЗИТЬ(Рег.Значение КАК Справочник.Номенклатура).Артикул,
    ТаблицаРегистра.ПрайсЛист.Владелец,
    ТаблицаРегистра.ПрайсЛист,
    ТаблицаРегистра.Наименование,
    ТаблицаРегистра.НаименованиеИностранное,
    ТаблицаРегистра.Цена * КурсыВалютСрезПоследних.Коэффициент,
    ВЫБОР
        КОГДА &amp;ПоВалютеОтчета
            ТОГДА &amp;ВалютаОтчета
        ИНАЧЕ ТаблицаРегистра.Валюта
    КОНЕЦ КАК Валюта,
    ТаблицаРегистра.Количество,
    ТаблицаРегистра.СрокПоставки,
    ТаблицаРегистра.ТегПозиции
ИЗ
    РегистрСведений.ПрайсЛистыКонтрагентовПравилаЗагрузки КАК Рег
        ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.ПрайсЛистыКонтрагентов КАК ТаблицаРегистра
        ПО Рег.ПрайсЛист = ТаблицаРегистра.ПрайсЛист
            И ((ВЫРАЗИТЬ(Рег.ОбъектПравила КАК СТРОКА(32))) = ТаблицаРегистра.КлючСтрокиПоставщика)
        ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалютСрезПоследних КАК КурсыВалютСрезПоследних
        ПО (ТаблицаРегистра.Валюта = КурсыВалютСрезПоследних.Валюта)
ГДЕ
    Рег.НазначениеПравила = ЗНАЧЕНИЕ(Перечисление.НазначениеПравилЗагрузки.КлючСтроки)
    И Рег.ИмяРеквизитаПрайсЛиста = "Номенклатура"
    И ВЫРАЗИТЬ(Рег.Значение КАК Справочник.Номенклатура).Ссылка В
            (ВЫБРАТЬ
                СписокНоменклатуры.Ссылка
            ИЗ
                СписокНоменклатуры)
{ГДЕ
    Рег.ПрайсЛист.Владелец.* КАК Контрагент,
    Рег.ПрайсЛист.* КАК ПрайсЛист}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    ВсеЦены.Номенклатура КАК Номенклатура,
    ВсеЦены.Производитель КАК Производитель,
    ВсеЦены.Артикул КАК Артикул,
    ВсеЦены.Контрагент КАК Контрагент,
    ВсеЦены.ПрайсЛист КАК ПрайсЛист,
    ВсеЦены.Наименование КАК Наименование,
    ВсеЦены.НаименованиеИностранное КАК НаименованиеИностранное,
    ВсеЦены.Валюта КАК Валюта,
    МИНИМУМ(ВсеЦены.СрокПоставки) КАК СрокПоставки,
    СРЕДНЕЕ(ВЫБОР КОГДА ВсеЦены.ПрайсЛист.ЦенаВключаетНДС ТОГДА 1 ИНАЧЕ (100 + ВсеЦены.ПрайсЛист.СтавкаНДС.Ставка) / 100 КОНЕЦ * ВсеЦены.Цена * (100 - ЕСТЬNULL(Скидки.Скидка, ЕСТЬNULL(СкидкиТегПозиции.Скидка, ЕСТЬNULL(СкидкиПроизводитель.Скидка, ЕСТЬNULL(СкидкиБазовые.Скидка, 0))))) / 100) КАК Цена,
    СУММА(ВсеЦены.Количество) КАК Количество
{ВЫБРАТЬ
    Контрагент.* КАК Контрагент,
    Номенклатура.* КАК Номенклатура,
    Артикул КАК Артикул,
    Производитель.* КАК Производитель,
    ПрайсЛист.* КАК ПрайсЛист,
    Наименование КАК Наименование,
    НаименованиеИностранное КАК НаименованиеИностранное,
    Цена КАК Цена,
    Валюта КАК Валюта,
    Количество КАК Количество,
    СрокПоставки КАК СрокПоставки}
ИЗ
    ВсеЦены КАК ВсеЦены
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПрайсЛистыКонтрагентовСкидки.СрезПоследних(
				&amp;ДатаОкончания,
				ПрайсЛист В
					(ВЫБРАТЬ РАЗЛИЧНЫЕ
						ВсеЦены.ПрайсЛист
					ИЗ
						ВсеЦены КАК ВсеЦены)) КАК Скидки
		ПО (НЕ Скидки.Отменена)
			И ВсеЦены.ПрайсЛист = Скидки.ПрайсЛист
			И ВсеЦены.ТегПозиции = Скидки.ТегПозиции
			И ВсеЦены.Производитель = Скидки.Производитель
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПрайсЛистыКонтрагентовСкидки.СрезПоследних(
				&amp;ДатаОкончания,
				ПрайсЛист В
					(ВЫБРАТЬ РАЗЛИЧНЫЕ
						ВсеЦены.ПрайсЛист
					ИЗ
						ВсеЦены КАК ВсеЦены)
					И ТегПозиции &lt;&gt; ""
					И Производитель = ЗНАЧЕНИЕ(Справочник.Производители.ПустаяСсылка)) КАК СкидкиТегПозиции
		ПО (НЕ Скидки.Отменена)
			И ВсеЦены.ПрайсЛист = СкидкиТегПозиции.ПрайсЛист
			И ВсеЦены.ТегПозиции = СкидкиТегПозиции.ТегПозиции
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПрайсЛистыКонтрагентовСкидки.СрезПоследних(
				&amp;ДатаОкончания,
				ПрайсЛист В
					(ВЫБРАТЬ РАЗЛИЧНЫЕ
						ВсеЦены.ПрайсЛист
					ИЗ
						ВсеЦены КАК ВсеЦены)
					И ТегПозиции = ""
					И Производитель &lt;&gt; ЗНАЧЕНИЕ(Справочник.Производители.ПустаяСсылка)) КАК СкидкиПроизводитель
		ПО (НЕ Скидки.Отменена)
			И ВсеЦены.ПрайсЛист = СкидкиПроизводитель.ПрайсЛист
			И ВсеЦены.Производитель = СкидкиПроизводитель.Производитель
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ПрайсЛистыКонтрагентовСкидки.СрезПоследних(
				&amp;ДатаОкончания,
				ПрайсЛист В
						(ВЫБРАТЬ РАЗЛИЧНЫЕ
							ВсеЦены.ПрайсЛист
						ИЗ
							ВсеЦены КАК ВсеЦены)
					И ТегПозиции = ""
					И Производитель = ЗНАЧЕНИЕ(Справочник.Производители.ПустаяСсылка)) КАК СкидкиБазовые
		ПО (НЕ СкидкиБазовые.Отменена)
			И ВсеЦены.ПрайсЛист = СкидкиБазовые.ПрайсЛист

СГРУППИРОВАТЬ ПО
    ВсеЦены.Номенклатура,
    ВсеЦены.Производитель,
    ВсеЦены.Контрагент,
    ВсеЦены.Артикул,
    ВсеЦены.ПрайсЛист,
    ВсеЦены.Наименование,
    ВсеЦены.НаименованиеИностранное,
    ВсеЦены.Валюта</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
	<totalField>
		<dataPath>Цена</dataPath>
		<expression>Среднее(Цена)</expression>
		<group>Валюта</group>
		<group>Контрагент</group>
		<group>Номенклатура</group>
		<group>ПрайсЛист</group>
		<group>Производитель</group>
	</totalField>
	<totalField>
		<dataPath>Количество</dataPath>
		<expression>Сумма(Количество)</expression>
		<group>Валюта</group>
		<group>Контрагент</group>
		<group>Номенклатура</group>
		<group>ПрайсЛист</group>
		<group>Производитель</group>
	</totalField>
	<totalField>
		<dataPath>СрокПоставки</dataPath>
		<expression>Минимум(СрокПоставки)</expression>
		<group>Валюта</group>
		<group>Контрагент</group>
		<group>Номенклатура</group>
		<group>ПрайсЛист</group>
		<group>Производитель</group>
	</totalField>
	<parameter>
