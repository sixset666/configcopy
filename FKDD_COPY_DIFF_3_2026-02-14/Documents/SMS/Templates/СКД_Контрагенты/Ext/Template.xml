		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	Контрагенты.Ссылка,
	ДОБАВИТЬКДАТЕ(ДОБАВИТЬКДАТЕ(ДАТАВРЕМЯ(1, 1, 1), МЕСЯЦ, МЕСЯЦ(Контрагенты.ДатаРождения) - 1), ДЕНЬ, ДЕНЬ(Контрагенты.ДатаРождения) - 1) КАК ДеньРождения,
	Контрагенты.ВидКонтрагента,
	Контрагенты.РекламныйИсточник,
	Контрагенты.Сотрудник,
	Контрагенты.Филиал,
	Контрагенты.ДатаРегистрации,
	Контрагенты.Пол,
	Контрагенты.СогласиеНаПолучениеSMS
ИЗ
	Справочник.Контрагенты КАК Контрагенты
ГДЕ
	Контрагенты.ЭтоГруппа = ЛОЖЬ
	И Контрагенты.ПометкаУдаления = ЛОЖЬ</query>
