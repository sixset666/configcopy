// Процедура заполнения предопределенных элементов
Процедура ЗаполнитьПредопределенныеЭлементы(ЗаполнятьВсе=Ложь) Экспорт
	ВидРемонта=Справочники.ВидыРемонта.ДополнительноеОборудование.ПолучитьОбъект();
	Если обЗначениеНеЗаполнено(ВидРемонта.ТипРемонта) Тогда
		ВидРемонта.ТипРемонта=Перечисления.ТипыРемонта.Платный;
		Попытка
			ВидРемонта.Записать();
			Сообщить("Справочник ""Виды ремонта"". Обновлен элемент <"+СокрЛП(ВидРемонта)+">");
		Исключение
			Сообщить("Справочник ""Виды ремонта"". Ошибка записи элемента <"+СокрЛП(ВидРемонта)+">",СтатусСообщения.Важное);
		КонецПопытки; 
	КонецЕсли; 
		
	ВидРемонта=Справочники.ВидыРемонта.КомплектацияАвтомобиля.ПолучитьОбъект();
	Если обЗначениеНеЗаполнено(ВидРемонта.ТипРемонта) Тогда
		ВидРемонта.ТипРемонта=Перечисления.ТипыРемонта.Бесплатный;
		Попытка
			ВидРемонта.Записать();
			Сообщить("Справочник ""Виды ремонта"". Обновлен элемент <"+СокрЛП(ВидРемонта)+">");
		Исключение
			Сообщить("Справочник ""Виды ремонта"". Ошибка записи элемента <"+СокрЛП(ВидРемонта)+">",СтатусСообщения.Важное);
		КонецПопытки; 
	КонецЕсли; 
КонецПроцедуры
