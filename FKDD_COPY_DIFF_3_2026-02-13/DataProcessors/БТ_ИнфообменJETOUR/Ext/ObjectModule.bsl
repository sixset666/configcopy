Функция ДоступенЛиДляВыгрузки(ЗаказНаряд) Экспорт
	
	Если 1=2 Тогда
		ЗаказНаряд = Документы.ЗаказНаряд.СоздатьДокумент();
	КонецЕсли;
	
	Если ЗаказНаряд.НеВыгружатьИмпортеру Тогда
		Возврат Ложь;
	КонецЕсли;
	
	НаимРегистраСоответствий = ПолучитьИмяРегистра(ПолучитьРегистрСопоставления());
	НаимРегистраСоответствийВидовРемонта = ПолучитьИмяРегистра(ПолучитьРегистрСопоставленияВидовРемонта());
	НаимРегистраСоответствийПодразделений = ПолучитьИмяРегистра(ПолучитьРегистрСопоставленияПодразделений());

	Запрос = Новый Запрос("ВЫБРАТЬ
		                      |	БТ_СопоставлениеПодразделений.Подразделение КАК Подразделение
		                      |ИЗ
		                      |	РегистрСведений."+НаимРегистраСоответствийПодразделений+" КАК БТ_СопоставлениеПодразделений
		                      |ГДЕ
		                      |	БТ_СопоставлениеПодразделений.Подразделение = &Подразделение
		                      |;
		                      |
		                      |////////////////////////////////////////////////////////////////////////////////
		                      |ВЫБРАТЬ
		                      |	БТ_СопоставлениеДанных.Период КАК Период
		                      |ИЗ
		                      |	РегистрСведений."+НаимРегистраСоответствий+" КАК БТ_СопоставлениеДанных
		                      |ГДЕ
		                      |	БТ_СопоставлениеДанных.Тип = &Тип
		                      |	И БТ_СопоставлениеДанных.Данные1С = &Данные1С
		                      |;
		                      |
		                      |////////////////////////////////////////////////////////////////////////////////
		                      |ВЫБРАТЬ
		                      |	БТ_СопоставлениеВидовРемонта.ВидРемонта КАК ВидРемонта,
		                      |	БТ_СопоставлениеВидовРемонта.ДанныеИнфообмена КАК ДанныеИнфообмена,
		                      |	БТ_СопоставлениеВидовРемонта.ТипОплатыИнфообмен КАК ТипОплатыИнфообмен
		                      |ИЗ
		                      |	РегистрСведений."+НаимРегистраСоответствийВидовРемонта+" КАК БТ_СопоставлениеВидовРемонта
		                      |ГДЕ
		                      |	БТ_СопоставлениеВидовРемонта.ВидРемонта = &ВидРемонта");
	Запрос.УстановитьПараметр("Тип", "Бренд");
	Запрос.УстановитьПараметр("Данные1С", ЗаказНаряд.Автомобиль.Модель.Родитель);
	СтрокаДляПодмены = ПодменяемыеДанные.Найти(ЗаказНаряд.ПодразделениеКомпании, "ПодменяемоеЗначение");
	
	Если СтрокаДляПодмены <> Неопределено Тогда
		Запрос.УстановитьПараметр("Подразделение", СтрокаДляПодмены.ЗначениеПодмены);			
	Иначе
		Запрос.УстановитьПараметр("Подразделение", ЗаказНаряд.ПодразделениеКомпании);
	КонецЕсли;
	Запрос.УстановитьПараметр("ВидРемонта", ЗаказНаряд.ВидРемонта);

	Пакет = Запрос.ВыполнитьПакет();
	
	ПроверкаПоСопоставлениюПодразделения = Не Пакет[0].Пустой();
	
	// При определении конкретной учетной записи для данного инфообмена пропускаем проверку по сопоставлению Профиль/Подразделение
	Если ЗначениеЗаполнено(ПолучитьКонстантноеЗначение("ТолькоПрофильПодразделения")) Тогда
		ПроверкаПоСопоставлениюПодразделения = Истина;
	КонецЕсли;
	
	Если ПроверкаПоСопоставлениюПодразделения и Не пакет[1].Пустой() И НЕ Пакет[2].пустой() Тогда
		Возврат Истина;
	Иначе
		Возврат Ложь;
	КонецЕсли;

КонецФункции
// Заявка на ремонт
Функция ДоступенЛиДляВыгрузкиЗнР(ЗнР_Ссылка)
	
	Если 1=2 Тогда
		ЗнР_Ссылка = Документы.ЗаявкаНаРемонт.СоздатьДокумент();
	КонецЕсли;

	//Проверим что автомобиль указан из справочника
	Если ЗнР_Ссылка.Автомобиль = Неопределено Или ТипЗнч(ЗнР_Ссылка.Автомобиль) <> Тип("СправочникСсылка.Автомобили") Тогда
		Возврат Ложь;
	КонецЕсли;

	//Проверим хозоперацию
	Если ЗнР_Ссылка.ХозОперация.Наименование <> "План ремонта" Тогда
		Возврат Ложь;
	КонецЕсли;
	//

	//Проверим что таблица планирования заполнена
	Если ЗнР_Ссылка.Планирование.Количество() = 0 Тогда
		Возврат Ложь;
	КонецЕсли;
	//
	
	//Проверим что все записи планирования для будущего
	Для каждого СтрокаПланирования Из ЗнР_Ссылка.Планирование цикл  
		Если СтрокаПланирования.НачалоВыполнения  <= ТекущаяДатаСеанса() Тогда 
			Возврат Ложь;
		КонецЕсли;
	КонецЦикла;
	//
	
	НаимРегистраСоответствий = ПолучитьИмяРегистра(ПолучитьРегистрСопоставления());
	НаимРегистраСоответствийВидовРемонта = ПолучитьИмяРегистра(ПолучитьРегистрСопоставленияВидовРемонта());
	НаимРегистраСоответствийПодразделений = ПолучитьИмяРегистра(ПолучитьРегистрСопоставленияПодразделений());

	Запрос = Новый Запрос("ВЫБРАТЬ
		                      |	БТ_СопоставлениеПодразделений.Подразделение КАК Подразделение
		                      |ИЗ
		                      |	РегистрСведений."+НаимРегистраСоответствийПодразделений+" КАК БТ_СопоставлениеПодразделений
		                      |ГДЕ
		                      |	БТ_СопоставлениеПодразделений.Подразделение = &Подразделение
		                      |;
		                      |
		                      |////////////////////////////////////////////////////////////////////////////////
		                      |ВЫБРАТЬ
		                      |	БТ_СопоставлениеДанных.Период КАК Период
		                      |ИЗ
		                      |	РегистрСведений."+НаимРегистраСоответствий+" КАК БТ_СопоставлениеДанных
		                      |ГДЕ
		                      |	БТ_СопоставлениеДанных.Тип = &Тип
		                      |	И БТ_СопоставлениеДанных.Данные1С = &Данные1С
		                      |;
		                      |
		                      |////////////////////////////////////////////////////////////////////////////////
		                      |ВЫБРАТЬ
		                      |	БТ_СопоставлениеВидовРемонта.ВидРемонта КАК ВидРемонта,
		                      |	БТ_СопоставлениеВидовРемонта.ДанныеИнфообмена КАК ДанныеИнфообмена,
		                      |	БТ_СопоставлениеВидовРемонта.ТипОплатыИнфообмен КАК ТипОплатыИнфообмен
		                      |ИЗ
		                      |	РегистрСведений."+НаимРегистраСоответствийВидовРемонта+" КАК БТ_СопоставлениеВидовРемонта
		                      |ГДЕ
		                      |	БТ_СопоставлениеВидовРемонта.ВидРемонта = &ВидРемонта
							  |	И БТ_СопоставлениеВидовРемонта.ВыгружатьДляЖурналаЗаписи = ИСТИНА");
	Запрос.УстановитьПараметр("Тип", "Модель");
	Запрос.УстановитьПараметр("Данные1С", ЗнР_Ссылка.Автомобиль.Модель);
	Запрос.УстановитьПараметр("Подразделение", ЗнР_Ссылка.ПодразделениеКомпании);
	Запрос.УстановитьПараметр("ВидРемонта", ЗнР_Ссылка.ВидРемонта);

	Пакет = Запрос.ВыполнитьПакет();
	
	ПроверкаПоСопоставлениюПодразделения = Не Пакет[0].Пустой();
	
	// При определении конкретной учетной записи для данного инфообмена пропускаем проверку по сопоставлению Профиль/Подразделение
	Если ЗначениеЗаполнено(ПолучитьКонстантноеЗначение("ТолькоПрофильПодразделения")) Тогда
		ПроверкаПоСопоставлениюПодразделения = Истина;
	КонецЕсли;
	
	Если ПроверкаПоСопоставлениюПодразделения и Не пакет[1].Пустой() И НЕ Пакет[2].пустой() Тогда
		Возврат Истина;
	Иначе
		Возврат Ложь;
	КонецЕсли;
	
КонецФункции
Процедура ЗаписатьЗнРДляВыгрузки(ЗнР_Ссылка, ДопСвойства=Неопределено) Экспорт

	НаборЗаписей = ПолучитьРегистрСопоставления().СоздатьНаборЗаписей();
	НаборЗаписей.Отбор.Тип.Установить("ПланРемонта");
	НаборЗаписей.Отбор.Данные1С.Установить(ЗнР_Ссылка);
	НаборЗаписей.Прочитать();
	Если НаборЗаписей.Количество() = 0 Тогда
		// Значит ЗнР еще нет в регистре
		// Проверим что Заявка не отменена
		ЗаявкаОтменена = (ЗнР_Ссылка.Состояние = Перечисления.СостоянияЗаявкиНаРемонт.Отклонено ИЛИ ЗнР_Ссылка.ПометкаУдаления);
		Если ЗаявкаОтменена Тогда
			Возврат;
		Иначе
			Мен = ПолучитьРегистрСопоставления().СоздатьМенеджерЗаписи();
			Мен.Период = ТекущаяДатаСеанса();
			Мен.Тип = "ПланРемонта";
			Мен.Данные1С = ЗнР_Ссылка;
			Мен.Записать();	
		КонецЕсли;
	Иначе
		// Значит ЗнР уже существует в регистре
		ТекущаяЗапись = НаборЗаписей[0];
		// Проверим выгружен ли он уже
		Если НЕ ЗначениеЗаполнено(ТекущаяЗапись.ID) Тогда
			// ЗнР еще не выгружен поэтому не будем трогать запись
			Возврат;
		ИначеЕсли ТекущаяЗапись.ТребуетсяОбновление = Истина Тогда
			// Для ЗнР уже стоит признак ТребуетсяОбновление, значит он уже в очереди на выгрузку
			Возврат;
		Иначе
			// ЗнР уже выгружен, проверим что есть ли доп свойства для проверки изменения
			ДанныеДоЗаписиОбъекта = Неопределено;
			Если ДопСвойства <> Неопределено И ДопСвойства.Свойство("ДанныеДляПроверкиЗнР" + ПолучитьНаименованиеИнфообмена(), ДанныеДоЗаписиОбъекта) Тогда
				// Проверим изменились ли данные
				ИзмененыДанные = Ложь;

				// Получим массив реквизитов для проверки
				МассивРеквизитов = ПолучитьКонстантноеЗначение("МассивРеквизитовДляПроверкиЗнР");

				// Проверим каждый реквизит на изменение
				Для Каждого Реквизит Из МассивРеквизитов Цикл
					// проверим что такой реквизит существует в структуре до записи
					Если ДанныеДоЗаписиОбъекта.Свойство(Реквизит) Тогда
						// определим тип реквизита
						Если ТипЗнч(ДанныеДоЗаписиОбъекта[Реквизит]) = Тип("ТаблицаЗначений") Тогда
							// Табличная часть
							Если НЕ КоллекцииИдентичны_Изолированн(ДанныеДоЗаписиОбъекта[Реквизит], ЗнР_Ссылка[Реквизит].Выгрузить()) Тогда
								ИзмененыДанные = Истина;
								Прервать;
							КонецЕсли;
						Иначе
							// Реквизит
							Если ДанныеДоЗаписиОбъекта[Реквизит] <> ЗнР_Ссылка[Реквизит] Тогда
								ИзмененыДанные = Истина;
								Прервать;
							КонецЕсли;
						КонецЕсли;
					КонецЕсли;
				КонецЦикла;
				
				// Если данные изменены, то установим признак ТребуетсяОбновление
				Если ИзмененыДанные Тогда
					ТекущаяЗапись.ТребуетсяОбновление = Истина;
					НаборЗаписей.Записать();
				КонецЕсли;
			Иначе
				// Если доп свойств нет, то сообщим что логика проверки изменений в документе нарушена но делать ничего не будем
				Сообщить("Попытка записи ЗнР в регистр сопоставления без данных для проверки изменений. Логика нарушена.");
				Возврат;
			КонецЕсли;
				
		КонецЕсли;
	КонецЕсли;

КонецПроцедуры
