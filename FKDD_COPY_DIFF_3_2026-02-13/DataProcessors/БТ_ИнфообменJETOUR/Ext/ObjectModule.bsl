Процедура ВыгрузитьВсеЗаявкиНаРемонт() Экспорт
	// Основная процедура выгрузки всех заявок на ремонт
	
	// Проверяем, что выгрузка заявок на ремонт включена
	Если НЕ ПолучитьВыгружатьЛиЗаписиНаРемонт() Тогда
		Возврат;
	КонецЕсли;
	
	// Получаем все невыгруженные заявки на ремонт
	Выборка = ПолучитьВыборкуВсехНеВыгрЗаявокНаРемонт();
	
	Пока Выборка.Следующий() Цикл
		Ошибка = "";
		
		// Устанавливаем соединение по профилю заявки на ремонт
		Если УстановитьСоединениеПоПрофилюЗаказНаряда(Выборка.Данные1С.ПодразделениеКомпании, Ошибка) Тогда
			// Выгружаем заявку на ремонт
			ИдЗаявки = ВыгрузитьЗаявкуНаРемонт(Выборка.Данные1С, Ошибка, Выборка.ID);
		КонецЕсли;
		
		// Записываем результат выгрузки в регистр сопоставления
		ДобавитьВыгруженныеДанные(Выборка.Данные1С, "ПланРемонта", ИдЗаявки, Ошибка);
	КонецЦикла;
КонецПроцедуры
Процедура ВыгрузитьОднуЗаявкуНаРемонт(ОднаЗаявка) Экспорт
	// Процедура выгрузки одной конкретной заявки на ремонт
	
	// Проверяем, что выгрузка заявок на ремонт включена
	Если НЕ ПолучитьВыгружатьЛиЗаписиНаРемонт() Тогда
		Сообщить("Выгрузка заявок на ремонт отключена");
		Возврат;
	КонецЕсли;
		
	// Получаем заявку для выгрузки
	Выборка = ПолучитьВыборкуНеВыгрЗаявокНаРемонтПоОдной(ОднаЗаявка);
	
	Если Выборка.Количество() = 0 Тогда
		Сообщить("Заявка на ремонт " + ОднаЗаявка + " не найдена в регистре сопоставления, или не требует обновления");
		Возврат;
	КонецЕсли;
	
	Пока Выборка.Следующий() Цикл
		Ошибка = "";
		
		// Устанавливаем соединение по профилю заявки на ремонт
		Если УстановитьСоединениеПоПрофилюЗаказНаряда(Выборка.Данные1С.ПодразделениеКомпании, Ошибка) Тогда
			// Выгружаем заявку на ремонт
			ИдЗаявки = ВыгрузитьЗаявкуНаРемонт(Выборка.Данные1С, Ошибка, Выборка.ID);
		КонецЕсли;
		
		// Записываем результат выгрузки в регистр сопоставления
		ДобавитьВыгруженныеДанные(Выборка.Данные1С, "ПланРемонта", ИдЗаявки, Ошибка);
		Прервать; // Выгружаем только одну заявку
	КонецЦикла;
КонецПроцедуры
Функция ВыгрузитьЗаявкуНаРемонт(Заявка, Ошибка, ИдОбновления = 0) Экспорт
	// Основная функция выгрузки документа "Заявка на ремонт" и ее графика на портал
	Если 1 = 2 Тогда
		Заявка = Документы.ЗаявкаНаРемонт.ПустаяСсылка();
	КонецЕсли;

	ФорматДатаВремя = "ДФ='dd.MM.yyyy HH:mm'";
	
	// Преобразуем ИД обновления к строке для использования в URL
	Если ЗначениеЗаполнено(ИдОбновления) Тогда
		ИдОбновленияСтрока = СтрЗаменить(ИдОбновления, Символы.НПП, "");
	КонецЕсли;

	// Определяем клиента (контрагента) и при необходимости выгружаем его
	ИдКлиента = Неопределено;
	Если ЗначениеЗаполнено(Заявка.Заказчик) Тогда
		ИдКлиента = НайтиВыгруженныйОбъект(Заявка.Заказчик, ПрофильПодключения);
		Если Не ЗначениеЗаполнено(ИдКлиента) Тогда
			Ошибка = "";
			ИдКлиента = ВыгрузитьКонтрагента(Заявка.Заказчик, Неопределено, Ошибка);
			ДобавитьВыгруженныеДанные(Заявка.Заказчик, "Контрагент", ИдКлиента, Ошибка, ПрофильПодключения);
			Если Не ЗначениеЗаполнено(ИдКлиента) Тогда
				Ошибка = "Не сопоставлен контрагент";
				Возврат 0;
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;

	// Определяем автомобиль и при необходимости выгружаем его
	ИдАвто = НайтиВыгруженныйОбъект(Заявка.Автомобиль);
	Если Не ЗначениеЗаполнено(ИдАвто) Тогда
		Ошибка = "";
		ИдАвто = ВыгрузитьАвтомобиль(Заявка.Автомобиль, Ошибка);
		Если ЗначениеЗаполнено(ИдАвто) ИЛИ НЕ ПустаяСтрока(Ошибка) Тогда
			ДобавитьВыгруженныеДанные(Заявка.Автомобиль, "Автомобиль", ИдАвто, Ошибка);
		КонецЕсли;
		Если Не ЗначениеЗаполнено(ИдАвто) Тогда
			Ошибка = "Не сопоставлен автомобиль";
			Возврат 0;
		КонецЕсли;
	Иначе	
		// При необходимости обновим пробег автомобиля
		Если ОбновитьАвтомобиль = Истина Тогда
			ОбновитьАвтомобиль(Заявка.Автомобиль, Ошибка, ИдАвто);
		КонецЕсли;
	КонецЕсли;

	// Контактное лицо
	КонтактноеЛицо = ПолучитьКонтактноеЛицо(Заявка.Заказчик);
	ИдКонтакта = "";
	Если ЗначениеЗаполнено(КонтактноеЛицо) И КонтактноеЛицо.ФормаСобственности = Перечисления.ФормыСобственности.ЧастноеЛицо Тогда
		ИдКонтакта = НайтиВыгруженныйОбъект(КонтактноеЛицо, ПрофильПодключения);
		Если Не ЗначениеЗаполнено(ИдКонтакта) Тогда
			ИдКонтакта = ВыгрузитьКонтрагента(КонтактноеЛицо, Неопределено, Ошибка);
			Если ЗначениеЗаполнено(ИдКонтакта) Тогда
				ДобавитьВыгруженныеДанные(КонтактноеЛицо, "Контрагент", ИдКонтакта, Ошибка, ПрофильПодключения);
			Иначе
				ИдКонтакта = "";
			КонецЕсли;
		КонецЕсли;
	Иначе
		ИдКонтакта = "";
	КонецЕсли;

	// Формируем базовую структуру заявки на ремонт
	Структура = Новый Структура;
	Структура.Вставить("code", СокрЛП(Заявка.Номер));
	Структура.Вставить("status", ?(Заявка.Состояние = Перечисления.СостоянияЗаявкиНаРемонт.Отклонено ИЛИ Заявка.ПометкаУдаления, "2", "1"));
	Структура.Вставить("occupying_type", "1"); // Всегда запись
	Если ЗначениеЗаполнено(ИдКлиента) Тогда
		Структура.Вставить("client_id", ИдКлиента);
	КонецЕсли;
	Структура.Вставить("car_id", ИдАвто);
	Структура.Вставить("contact_person_id", ИдКонтакта);

	// Разберем планирование, чтобы определить приемку/работы/выдачу и собрать станции
	МассивСтанций = Новый Массив;
	
	// Данные приемки
	МастерДляПриемки = Неопределено;
	ИДМастераПриемки = Неопределено;
	НачалоПриемки = Неопределено;
	КонецПриемки = Неопределено;
	
	// Данные Выдачи
	МастерДляВыдачи = Неопределено;
	ИДМастераВыдачи = Неопределено;
	НачалоВыдачи = Неопределено;
	КонецВыдачи = Неопределено;
	
	// Определим автоработы приемки и выдачи
	АвтоработаПриемки = ПолучитьКонстантноеЗначение("АвтоработаПриемаАвтомобиля");
	АвтоработаВыдачи = ПолучитьКонстантноеЗначение("АвтоработаВыдачиАвтомобиля"); 
	
	Если АвтоработаПриемки = Справочники.Автоработы.ПустаяСсылка() ИЛИ АвтоработаВыдачи = Справочники.Автоработы.ПустаяСсылка() Тогда
		Ошибка = "Не указана авторабота приемки или выдачи. Проверьте константы инфообмена.";
		Возврат 0;
	КонецЕсли;

	// Перед тем как выгружать проверим что есть приемка и есть выдача
	МассивСтрокПриемки = Заявка.Планирование.НайтиСтроки(Новый Структура("Авторабота", АвтоработаПриемки));
	МассивСтрокВыдачи = Заявка.Планирование.НайтиСтроки(Новый Структура("Авторабота", АвтоработаВыдачи));
	Если МассивСтрокПриемки.Количество() = 0 ИЛИ МассивСтрокВыдачи.Количество() = 0 Тогда
		Ошибка = "Не указана приемка или выдача. Проверьте планирование заявки.";
		Возврат 0;
	КонецЕсли;
	
	// Получим начальное время и конечное время приемки и выдачи
	ТаблицаВремениПриемки = Заявка.Планирование.Выгрузить(МассивСтрокПриемки, "НачалоВыполнения,ОкончаниеВыполнения");
	ТаблицаВремениВыдачи = Заявка.Планирование.Выгрузить(МассивСтрокВыдачи, "НачалоВыполнения,ОкончаниеВыполнения");
	// Сортируем чтобы минимальное начальное время было первой строкой
	ТаблицаВремениПриемки.Сортировать("НачалоВыполнения Возр");
	НачалоПриемки = ТаблицаВремениПриемки.Получить(0).НачалоВыполнения;
	// Сортируем чтобы максимальное конечное время было первой строкой
	ТаблицаВремениПриемки.Сортировать("ОкончаниеВыполнения Убыв");
	КонецПриемки = ТаблицаВремениПриемки.Получить(0).ОкончаниеВыполнения;

	// Теперь тоже самое для выдачи
	ТаблицаВремениВыдачи.Сортировать("НачалоВыполнения Возр");
	НачалоВыдачи = ТаблицаВремениВыдачи.Получить(0).НачалоВыполнения;
	ТаблицаВремениВыдачи.Сортировать("ОкончаниеВыполнения Убыв");
	КонецВыдачи = ТаблицаВремениВыдачи.Получить(0).ОкончаниеВыполнения;

	// Определим статус заявки
	ЗаявкаОтменена = (Заявка.Состояние = Перечисления.СостоянияЗаявкиНаРемонт.Отклонено ИЛИ Заявка.ПометкаУдаления);
	
	Для Каждого СтрокаПлан Из Заявка.Планирование Цикл
		// Пока не вижу смысла по виду использования цеха определять приемка ли это или просто запланированная работа
		// Поэтому оставлю данную строку для будущих улучшений механизма
		ВидИспользования = обПолучитьЗначениеСвойства(СтрокаПлан.РабочееМесто, ПланыВидовХарактеристик.СвойстваОбъектов.ВидИспользованияРабочегоМеста, Перечисления.ВидыИспользованияРабочихМест.НеУчаствуетВПланировании);

		// Цех нужен только для работ. Для приемки и выдачи он не нужен.
		Если НЕ СтрокаПлан.Авторабота = АвтоработаПриемки И НЕ СтрокаПлан.Авторабота = АвтоработаВыдачи Тогда
			// Определим цех по цеху
			ИДЦех = НайтиВыгруженныйОбъект(СтрокаПлан.РабочееМесто, ПрофильПодключения, "Пост");
			Если Не ЗначениеЗаполнено(ИДЦех) Тогда
				Ошибка = "Не сопоставлен цех";
				Возврат 0;
			КонецЕсли;
		КонецЕсли;

		// Обработка по условиям нужна для гибкости механизма выгрузки
		Если СтрокаПлан.Авторабота = АвтоработаПриемки Тогда
			// Обрабатываем приемку:

			// Определим мастера-приемщика по следующим правилам:
			// 1. Мастер заполнен как исполнитель работы ИНАЧЕ
			// 2. Мастер заполнен как мастер цеха ИНАЧЕ
			// 3. Мастер заполнен как диспетчер
			Если ЗначениеЗаполнено(СтрокаПлан.Исполнитель) Тогда
				МастерДляПриемки = СтрокаПлан.Исполнитель;
			ИначеЕсли ЗначениеЗаполнено(СтрокаПлан.РабочееМесто.Мастер) Тогда
				МастерДляПриемки = СтрокаПлан.РабочееМесто.Мастер;
			ИначеЕсли ЗначениеЗаполнено(Заявка.Диспетчер) Тогда
				МастерДляПриемки = Заявка.Диспетчер;
			Иначе
				Ошибка = "Не определен мастер-приемщик";
				Возврат 0;
			КонецЕсли;

			// Выполним поиск мастера в регистре выгруженных данных
			ИДМастераПриемки = НайтиВыгруженныйОбъект(МастерДляПриемки);
			Если Не ЗначениеЗаполнено(ИДМастераПриемки) Тогда
				Ошибка = "";
				ИДМастераПриемки = ВыгрузитьСотрудника(МастерДляПриемки, Ошибка, Истина);
				ДобавитьВыгруженныеДанные(МастерДляПриемки, "Сотрудник", ИДМастераПриемки, Ошибка);
				Если Не ЗначениеЗаполнено(ИДМастераПриемки) Тогда
					Ошибка = "Не сопоставлен мастер-приемщик. Ошибка выгрузки сотрудника.";
					Возврат 0;
				КонецЕсли;
			КонецЕсли;

			// Проверим график мастера на день приемки
			ИДГрафикаПриемки = НайтиВыгруженныйОбъект(МастерДляПриемки,, "ГрафикРаботы", Формат(СтрокаПлан.НачалоВыполнения, "ДФ=dd.MM.yyyy"));
			Если Не ЗначениеЗаполнено(ИДГрафикаПриемки) Тогда
				ОшибкаГрафика = "";
				ИДГрафикаПриемки = ВыгрузитьГрафикРаботы(ИДМастераПриемки, СтрокаПлан.НачалоВыполнения, ОшибкаГрафика);
				Если НЕ ПустаяСтрока(ОшибкаГрафика) Тогда 
					Ошибка = "Не выгружен график работы " + МастерДляПриемки + " за день " + Формат(СтрокаПлан.НачалоВыполнения, "ДФ=dd.MM.yyyy") + ": " + ОшибкаГрафика;
					Возврат 0;
				КонецЕсли;
				// Записываем сопоставление графика в регистр
				ДобавитьВыгруженныеДанные(МастерДляПриемки, "ГрафикРаботы", ИДГрафикаПриемки, ОшибкаГрафика,, Формат(СтрокаПлан.НачалоВыполнения, "ДФ=dd.MM.yyyy"));
			КонецЕсли;

		ИначеЕсли СтрокаПлан.Авторабота = АвтоработаВыдачи Тогда
			// Обрабатываем выдачу:

			// Определим мастера-выдачи по следующим правилам:
			// 1. Мастер заполнен как исполнитель работы ИНАЧЕ
			// 2. Мастер заполнен как мастер цеха ИНАЧЕ
			// 3. Мастер заполнен как диспетчер
			Если ЗначениеЗаполнено(СтрокаПлан.Исполнитель) Тогда
				МастерДляВыдачи = СтрокаПлан.Исполнитель;
			ИначеЕсли ЗначениеЗаполнено(СтрокаПлан.РабочееМесто.Мастер) Тогда
				МастерДляВыдачи = СтрокаПлан.РабочееМесто.Мастер;
			ИначеЕсли ЗначениеЗаполнено(Заявка.Диспетчер) Тогда
				МастерДляВыдачи = Заявка.Диспетчер;
			Иначе
				Ошибка = "Не определен мастер для выдачи автомобиля";
				Возврат 0;
			КонецЕсли;

			// Выполним поиск мастера в регистре выгруженных данных
			ИДМастераВыдачи = НайтиВыгруженныйОбъект(МастерДляВыдачи);
			Если Не ЗначениеЗаполнено(ИДМастераПриемки) Тогда
				Ошибка = "";
				ИДМастераВыдачи = ВыгрузитьСотрудника(МастерДляВыдачи, Ошибка, Истина);
				ДобавитьВыгруженныеДанные(МастерДляВыдачи, "Сотрудник", ИДМастераВыдачи, Ошибка);
				Если Не ЗначениеЗаполнено(ИДМастераВыдачи) Тогда
					Ошибка = "Не сопоставлен мастер для выдачи автомобиля. Ошибка выгрузки сотрудника.";
					Возврат 0;
				КонецЕсли;
			КонецЕсли;

			// Проверим график мастера на день выдачи
			ИДГрафикаВыдачи = НайтиВыгруженныйОбъект(МастерДляВыдачи,, "ГрафикРаботы", Формат(СтрокаПлан.НачалоВыполнения, "ДФ=dd.MM.yyyy"));
			Если Не ЗначениеЗаполнено(ИДГрафикаВыдачи) Тогда
				ОшибкаГрафика = "";
				ИДГрафикаВыдачи = ВыгрузитьГрафикРаботы(ИДМастераВыдачи, СтрокаПлан.НачалоВыполнения, ОшибкаГрафика);
				Если НЕ ПустаяСтрока(ОшибкаГрафика) Тогда
					Ошибка = "Не выгружен график работы " + МастерДляВыдачи + " за день " + Формат(СтрокаПлан.НачалоВыполнения, "ДФ=dd.MM.yyyy") + ": " + ОшибкаГрафика;
					Возврат 0;
				КонецЕсли;
				// Записываем сопоставление графика в регистр
				ДобавитьВыгруженныеДанные(МастерДляВыдачи, "ГрафикРаботы", ИДГрафикаВыдачи, ОшибкаГрафика,, Формат(СтрокаПлан.НачалоВыполнения, "ДФ=dd.MM.yyyy"));
			КонецЕсли;

		Иначе
			// Обрабатываем производственные работы:

			// Мастер и его график не требуется для производственных работ


			СтруктураСтанции = Новый Структура;
			СтруктураСтанции.Вставить("station_id", ИДЦех);
			СтруктураСтанции.Вставить("service_from", Формат(СтрокаПлан.НачалоВыполнения, ФорматДатаВремя));
			СтруктураСтанции.Вставить("service_to", Формат(СтрокаПлан.ОкончаниеВыполнения, ФорматДатаВремя));
			СтруктураСтанции.Вставить("request_reason", Строка(СтрокаПлан.Авторабота));
			СтруктураСтанции.Вставить("status", ?(ЗаявкаОтменена, "2", "1"));
			МассивСтанций.Добавить(СтруктураСтанции);
			
		КонецЕсли;
	КонецЦикла;

	// Укажем даты приема и выдачи автомобиля и их мастеров
	Структура.Вставить("acceptor_id", ИДМастераПриемки);
	Структура.Вставить("accept_from", Формат(НачалоПриемки, ФорматДатаВремя));
	Структура.Вставить("accept_to", Формат(КонецПриемки, ФорматДатаВремя));

	Структура.Вставить("issued_by_id", ИДМастераВыдачи);
	Структура.Вставить("issue_from", Формат(НачалоВыдачи, ФорматДатаВремя));
	Структура.Вставить("issue_to", Формат(КонецВыдачи, ФорматДатаВремя));

	// Добавим собранные станции в структуру
	Структура.Вставить("stations", МассивСтанций);

	// Отправим заявку: POST для новых, PUT для обновления
	Ошибка = "";
	Если ЗначениеЗаполнено(ИдОбновленияСтрока) Тогда
		ИД = ОтправитьЗапрос("api/service/maintenance-schedule/station-reschedule/" + ИдОбновленияСтрока, Структура, Ошибка, "PUT");
	Иначе
		ИД = ОтправитьЗапрос("api/service/maintenance-schedule", Структура, Ошибка);
	КонецЕсли;

	Возврат ИД;
КонецФункции
