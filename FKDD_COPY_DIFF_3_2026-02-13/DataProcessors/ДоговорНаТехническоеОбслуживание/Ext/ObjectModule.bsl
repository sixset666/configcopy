Функция Печать(Контрагент) Экспорт
	
	//Если Контрагент.ФормаСобственности <> Перечисления.ФормыСобственности.ЮридическоеЛицо Тогда
	//	Сообщить("Печатная форма предназначена только для юр лиц!");
	//	Возврат Неопределено;
	//КонецЕсли;
	
	ТабДокумент = Новый ТабличныйДокумент;
	
	нФормаВыбора = Справочники.Организации.ПолучитьФормуВыбора();  
	нФормаВыбора.Заголовок = "Укажите организацию";
	
	нФормаВыбора.Отбор.ПометкаУдаления.ВидСравнения = ВидСравнения.Равно;
	нФормаВыбора.Отбор.ПометкаУдаления.Значение = Ложь;
	нФормаВыбора.Отбор.ПометкаУдаления.Использование = Истина;
	
	Организация = нФормаВыбора.ОткрытьМодально();
	Если НЕ ЗначениеЗаполнено(Организация) Тогда
		Сообщить("Не выбрана организация!");
		Возврат Неопределено;
	КонецЕсли;
	
	нФормаВыбора = Справочники.ДоговорыВзаиморасчетов.ПолучитьФормуВыбора();  
	нФормаВыбора.Заголовок = "Укажите договор контрагента";
	
	нФормаВыбора.Отбор.Владелец.ВидСравнения = ВидСравнения.Равно;
	нФормаВыбора.Отбор.Владелец.Значение = Контрагент.Ссылка;
	нФормаВыбора.Отбор.Владелец.Использование = Истина; 
	
	нФормаВыбора.Отбор.Организация.ВидСравнения = ВидСравнения.Равно;
	нФормаВыбора.Отбор.Организация.Значение = Организация;
	нФормаВыбора.Отбор.Организация.Использование = Истина;
	
	нФормаВыбора.Отбор.ПометкаУдаления.ВидСравнения = ВидСравнения.Равно;
	нФормаВыбора.Отбор.ПометкаУдаления.Значение = Ложь;
	нФормаВыбора.Отбор.ПометкаУдаления.Использование = Истина;
	
	ДоговорВзаиморасчетов = нФормаВыбора.ОткрытьМодально();
	Если НЕ ЗначениеЗаполнено(ДоговорВзаиморасчетов) Тогда
		Сообщить("Не выбран договор контрагента!");
		Возврат Неопределено;
	КонецЕсли;
	
	нФормаВыбора = Справочники.Сотрудники.ПолучитьФормуВыбора();  
	нФормаВыбора.Заголовок = "Укажите контактное лицо исполнителя";
	
	нФормаВыбора.Отбор.ФлагУволен.ВидСравнения = ВидСравнения.Равно;
	нФормаВыбора.Отбор.ФлагУволен.Значение = Ложь;
	нФормаВыбора.Отбор.ФлагУволен.Использование = Истина; 
	
	нФормаВыбора.Отбор.ПометкаУдаления.ВидСравнения = ВидСравнения.Равно;
	нФормаВыбора.Отбор.ПометкаУдаления.Значение = Ложь;
	нФормаВыбора.Отбор.ПометкаУдаления.Использование = Истина;
	
	КонтактноеЛицоИсполнителя = нФормаВыбора.ОткрытьМодально();
	
	Ответ = Вопрос("Указать подписанта?", РежимДиалогаВопрос.ДаНет);
	
	Подписант = Неопределено;
	ПодписантЗаполненКорректно = Истина;
	
	Если Ответ = КодВозвратаДиалога.Да Тогда
		нФормаВыбора = Справочники.Сотрудники.ПолучитьФормуВыбора();  
		нФормаВыбора.Заголовок = "Укажите подписанта";
		
		нФормаВыбора.Отбор.ФлагУволен.ВидСравнения = ВидСравнения.Равно;
		нФормаВыбора.Отбор.ФлагУволен.Значение = Ложь;
		нФормаВыбора.Отбор.ФлагУволен.Использование = Истина; 
		
		нФормаВыбора.Отбор.ПометкаУдаления.ВидСравнения = ВидСравнения.Равно;
		нФормаВыбора.Отбор.ПометкаУдаления.Значение = Ложь;
		нФормаВыбора.Отбор.ПометкаУдаления.Использование = Истина;
		
		Подписант = нФормаВыбора.ОткрытьМодально();
	КонецЕсли;
	
	Макет = ПолучитьМакет("МакетДляЗаполненияПоКонтрагенту"); 
	
	КонтрагентСтр = ПолучитьИнформациюКонтрагента(Контрагент);
	Макет.Параметры.Заполнить(КонтрагентСтр);
	
	ДанныеДляПФ = новый Структура;
	ДанныеДляПФ.Вставить("ОрганизацияПолноеНаименование");
	ДанныеДляПФ.Вставить("ОрганизацияНаименование");
	ДанныеДляПФ.Вставить("Город");
	ДанныеДляПФ.Вставить("ФактАдресОрганизация");
	ДанныеДляПФ.Вставить("ЮрАдресОрганизация");
	ДанныеДляПФ.Вставить("ОГРНОрганизация");
	ДанныеДляПФ.Вставить("ИННКППОрганизация");
	ДанныеДляПФ.Вставить("РСОрганизация");
	ДанныеДляПФ.Вставить("КсчетОрганизация");
	ДанныеДляПФ.Вставить("БанкОрганизация");
	ДанныеДляПФ.Вставить("БикОрганизация");
	ДанныеДляПФ.Вставить("ТелефонОрганизация");
	 
	ДанныеДляПФ.Вставить("НомерДоговора");
	ДанныеДляПФ.Вставить("ДатаНачалаДоговора");
	ДанныеДляПФ.Вставить("ДатаОкончанияДоговора");
	ДанныеДляПФ.Вставить("ИсполнительВЛице");
	ДанныеДляПФ.Вставить("ПодписантСокр");
	                        
	ДанныеДляПФ.НомерДоговора = ДоговорВзаиморасчетов.НомерДоговора;
	ДанныеДляПФ.ДатаНачалаДоговора = Формат(ДоговорВзаиморасчетов.ДатаНачала,"ДФ='«dd» MMMM yyyy'")+" г.";
	
		
	
	Если ЗначениеЗаполнено(ДоговорВзаиморасчетов.ДатаКонца) Тогда
		ДанныеДляПФ.ДатаОкончанияДоговора = Формат(ДоговорВзаиморасчетов.ДатаКонца,"ДФ='«dd» MMMM yyyy'")+" г."; 
	Иначе
		ДанныеДляПФ.ДатаОкончанияДоговора = "___________";
	КонецЕсли; 
	
	ДанныеДляПФ.Вставить("АдресСервисаИсполнителя");
	ДанныеДляПФ.АдресСервисаИсполнителя = киПолучитьПредставлениеКИ(ДоговорВзаиморасчетов.Подразделение,Справочники.ВидыКонтактнойИнформации.АдресФактический);
	Если НЕ ЗначениеЗАполнено(ДанныеДляПФ.АдресСервисаИсполнителя) Тогда
		ДанныеДляПФ.АдресСервисаИсполнителя = "_________________________________________";	
	КонецЕсли;
	
	ДанныеДляПФ.Вставить("ИсполнительКонтактноеЛицо");
	Если ЗначениеЗаполнено(КонтактноеЛицоИсполнителя) Тогда
		ДанныеДляПФ.ИсполнительКонтактноеЛицо =  КонтактноеЛицоИсполнителя.Наименование;
	Иначе
		ДанныеДляПФ.ИсполнительКонтактноеЛицо = "_______________________________";
	КонецЕсли;
	
	Если Подписант <> Неопределено Тогда
		Должность = Подписант.Должность.Наименование;
		ПодписантДолжность = НРег(ПадежП(Должность, 2)); 		
		ПодписантФИО = Падеж(Подписант.Наименование, 2);
		Запрос = Новый Запрос;
		Запрос.Текст = 
		"ВЫБРАТЬ
		|   ПодтверждающиеДокументы.ДатаВыдачи КАК ДатаВыдачи,
		|   ПодтверждающиеДокументы.ВидПодтверждающегоДокумента КАК ВидПодтверждающегоДокумента,
		|   ПодтверждающиеДокументы.Номер КАК Номер
		|ИЗ
		|   Справочник.ПодтверждающиеДокументы КАК ПодтверждающиеДокументы
		|ГДЕ
		|   ПодтверждающиеДокументы.ВидПодтверждающегоДокумента = &ВидПодтверждающегоДокумента
		|   И ПодтверждающиеДокументы.Владелец = &Владелец
		|   И ПодтверждающиеДокументы.Текущий = ИСТИНА";
		Запрос.УстановитьПараметр("ВидПодтверждающегоДокумента", Перечисления.ВидыДокументов.Доверенность);
		Запрос.УстановитьПараметр("Владелец", Подписант);
		РезультатЗапроса = Запрос.Выполнить().Выгрузить();
		
		Если РезультатЗапроса.Количество() > 0 Тогда
			НазваниеДокумента = ПадежС("доверенность", 2); 			
			ПодтверждающийДокумент = НазваниеДокумента + " №" + Строка(РезультатЗапроса[0].Номер);
			ДатаПодтверждающегоДокумента = Формат(РезультатЗапроса[0].ДатаВыдачи,"ДЛФ=Д");
			ДанныеДляПФ.ИсполнительВЛице = ПодписантДолжность + " " + ПодписантФИО + ", действующего на основании " + ПодтверждающийДокумент + " от " + ДатаПодтверждающегоДокумента + "г."; 
			Массив = СтрРазделить(Подписант.Наименование, " ", Ложь);
			ДанныеДляПФ.ПодписантСокр = ?(Массив.Количество() > 0, Массив[0], "") 
			+ ?(Массив.Количество() > 1, " " + Лев(Массив[1], 1) + ".", "") 
			+ ?(Массив.Количество() > 2, " " + Лев(Массив[2], 1) + ".", "");
		Иначе
			Сообщить("Подтверждающий документ подписанта не найден!");
			ПодписантЗаполненКорректно = Ложь;
		КонецЕсли;
	Иначе
		ПодписантЗаполненКорректно = Ложь;	
	КонецЕсли;
	
	Если НЕ ПодписантЗаполненКорректно Тогда
		ДанныеДляПФ.ИсполнительВЛице = "генерального директора Капёнкина Игоря Вячеславовича, действующего на основании договора о передаче полномочий ЕИО управляющей организации от 10.01.2024 и Устава управляющего — ООО «УК Оптима»";
	КонецЕсли;
	
	Если ДанныеДляПФ.ПодписантСокр = Неопределено Тогда
    	ДанныеДляПФ.ПодписантСокр = "Капёнкин И.В."
	КонецЕсли;
	

	
	
	ДанныеДляПФ.Город = "Краснодар";
	ДанныеДляПФ.ОрганизацияНаименование = спПолучитьНаименование(Организация);
	ДанныеДляПФ.ОрганизацияПолноеНаименование = ПолучитьПолноеНаименованиеОрганизации(спПолучитьНаименование(Организация)); 
	
	ДанныеДляПФ.ФактАдресОрганизация = киПолучитьПредставлениеКИ(Организация,Справочники.ВидыКонтактнойИнформации.АдресФактический);
	ДанныеДляПФ.ЮрАдресОрганизация   = киПолучитьПредставлениеКИ(Организация,Справочники.ВидыКонтактнойИнформации.АдресЮридический);
	ДанныеДляПФ.ОГРНОрганизация      = СокрЛП(Организация.ОГРН);
	ДанныеДляПФ.ИННКППОрганизация    = СокрЛП(Организация.ИНН) + "/" + СокрЛП(Организация.КПП); 
	ДанныеДляПФ.РСОрганизация        = Организация.ОсновнойБанковскийСчет.НомерСчета;
	ДанныеДляПФ.КсчетОрганизация     = Организация.ОсновнойБанковскийСчет.Банк.КоррСчет;
	ДанныеДляПФ.БанкОрганизация      = Организация.ОсновнойБанковскийСчет.Банк.Наименование;
	ДанныеДляПФ.БикОрганизация       = Организация.ОсновнойБанковскийСчет.Банк.Код;
	ДанныеДляПФ.ТелефонОрганизация   = киПолучитьПредставлениеКИ(Организация,Перечисления.ТипыКонтактнойИнформации.Телефон);
	
	//ДанныеДляПФ.СуммаЗНиПрописью = СуммаИСуммаПрописью(ДокументОбъект.СуммаДокумента,ДокументОбъект);
	//ДанныеДляПФ.СуммаНДСЗНиПрописью = СуммаИСуммаПрописью(ДокументОбъект.Работы.Итог("СуммаНДС")+ДокументОбъект.Товары.Итог("СуммаНДС"),ДокументОбъект);
	//ДанныеДляПФ.ПроцентПредоплаты = ДанныеДляПФ.Договор.ПроцентПредоплаты;
	//ДанныеДляПФ.ДатаКонцаДоговора = ДанныеДляПФ.Договор.ДатаКонца;
	//ДанныеДляПФ.МастерПриемщик = ДокументОбъект.Мастер.ПолноеНаименование();
	
	
	Макет.Параметры.Заполнить(ДанныеДляПФ);
	
	НастроитьПоляИВыводКолонтитулов(ТабДокумент);
	
	ТабДокумент.Вывести(Макет);
	
	ТабДокумент.Показать();
	//ТабДокумент.Напечатать(РежимИспользованияДиалогаПечати.Использовать);
	Возврат ТабДокумент;	
КонецФункции

