Функция ИзвлечьРаботыИзЗНсИсполнителями (ДокументОбъект)
	ЗапросРаботы = новый Запрос;
	ЗапросРаботы.Текст = "ВЫБРАТЬ
	|	ЗаказНарядРаботы.Работа КАК НаименованиеРаботы,
	|	МАКСИМУМ(ЗаказНарядИсполнители.Исполнитель) КАК Исполнитель,
	|	ЗаказНарядРаботы.СуммаВсего КАК СтоимостьРаботы,
	|	ЗаказНарядРаботы.НомерСтроки КАК НомерСтроки,
	|	ЗаказНарядРаботы.Количество КАК Количество,
	|	ЗаказНарядРаботы.Нормочас КАК Нормочас,
	|	ЗаказНарядРаботы.Цена КАК Цена,
	|	ЗаказНарядРаботы.Сумма КАК Сумма,
	|	ЗаказНарядРаботы.СтавкаНДС КАК СтавкаНДС,
	|	ЗаказНарядРаботы.СуммаНДС КАК СуммаНДС,
	|	ЗаказНарядРаботы.ПроцентСкидки КАК ПроцентСкидки,
	|	ЗаказНарядРаботы.СуммаСкидки КАК СуммаСкидки,
	|	ЗаказНарядРаботы.СуммаВсего КАК СуммаВсего,
	|	ЗаказНарядРаботы.ПроцентСкидкиСтроки КАК ПроцентСкидкиСтроки,
	|	ЗаказНарядРаботы.СуммаСкидкиСтроки КАК СуммаСкидкиСтроки
	|ИЗ
	|	Документ.ЗаказНаряд.Работы КАК ЗаказНарядРаботы
	|		ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказНаряд.Исполнители КАК ЗаказНарядИсполнители
	|		ПО ЗаказНарядРаботы.Ссылка = ЗаказНарядИсполнители.Ссылка
	|			И ЗаказНарядРаботы.ИдентификаторРаботы = ЗаказНарядИсполнители.ИдентификаторРаботы
	|ГДЕ
	|	ЗаказНарядРаботы.Ссылка = &Ссылка
	|
	|СГРУППИРОВАТЬ ПО
	|	ЗаказНарядРаботы.Работа,
	|	ЗаказНарядРаботы.СуммаВсего,
	|	ЗаказНарядРаботы.НомерСтроки,
	|	ЗаказНарядРаботы.Количество,
	|	ЗаказНарядРаботы.Нормочас,
	|	ЗаказНарядРаботы.Цена,
	|	ЗаказНарядРаботы.Сумма,
	|	ЗаказНарядРаботы.СтавкаНДС,
	|	ЗаказНарядРаботы.СуммаНДС,
	|	ЗаказНарядРаботы.ПроцентСкидки,
	|	ЗаказНарядРаботы.СуммаСкидки,
	|	ЗаказНарядРаботы.ПроцентСкидкиСтроки,
	|	ЗаказНарядРаботы.СуммаСкидкиСтроки,
	|	ЗаказНарядРаботы.СуммаВсего
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтроки";
	
	ЗапросРаботы.УстановитьПараметр("Ссылка",ДокументОбъект.Ссылка);					 
	Возврат ЗапросРаботы.Выполнить().Выгрузить();
КонецФункции
Функция ОпределитьДоверенностьУстав(Знач Текст)
	Строки = СтрРазделить(Текст," ",ЛОЖЬ);
	Результат = "";
	Если Нрег(Строки[0]) = "доверенность" Тогда
		Возврат "Доверенности";
	ИначеЕсли Нрег(Строки[0]) = "устав" Тогда
		Возврат "Устава";
	Иначе
		Возврат "Устава";
	КонецЕсли;
КонецФункции
Функция ПолучитьКонтактноеЛицо(Контрагент)
	КонтактноеЛицоСтр = Новый Структура;
	КонтактноеЛицоСтр.Вставить("Контрагент");
	КонтактноеЛицоСтр.Вставить("НаОсновании");
	КонтактноеЛицоСтр.Вставить("Должность");
	
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	КонтактныеЛицаКонтрагентов.Ведомый КАК Ведомый,
	|	КонтактныеЛицаКонтрагентов.Должность КАК Должность,
	|	КонтактныеЛицаКонтрагентов.Доверенность КАК Доверенность
	|ИЗ
	|	РегистрСведений.КонтактныеЛицаКонтрагентов КАК КонтактныеЛицаКонтрагентов
	|ГДЕ
	|	КонтактныеЛицаКонтрагентов.Ведущий = &Ведущий
	|	И КонтактныеЛицаКонтрагентов.ВидОтношений = &ВидОтношений
	|	И КонтактныеЛицаКонтрагентов.Основной = Истина";
	
	Запрос.УстановитьПараметр("Ведущий", Контрагент);
	Запрос.УстановитьПараметр("ВидОтношений", Справочники.ВидыВзаимоотношений.Контактноелицо);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Если  ВыборкаДетальныеЗаписи.Следующий() Тогда
		КонтактноеЛицоСтр.Контрагент =  ВыборкаДетальныеЗаписи.Ведомый;
		Если Значениезаполнено(ВыборкаДетальныеЗаписи.Доверенность) Тогда
			КонтактноеЛицоСтр.НаОсновании = ОпределитьДоверенностьУстав(ВыборкаДетальныеЗаписи.Доверенность);	
		Иначе
			КонтактноеЛицоСтр.НаОсновании = "Устава";	
		КонецЕсли;
		Если Значениезаполнено(ВыборкаДетальныеЗаписи.Должность.Наименование) Тогда
			КонтактноеЛицоСтр.Должность = ВыборкаДетальныеЗаписи.Должность.Наименование;
		КонецЕсли;
	КонецЕсли; 
	Возврат КонтактноеЛицоСтр;
КонецФункции
