//}
//	
//{ИТОГИ ПО
//	Автомобиль,
//	ВидОборудования,
//	Номенклатура
//}
//	
//ИТОГИ
//	СУММА(Количество)
//ПО
//	Автомобиль,
//	ВидОборудования,
//	Номенклатура
</query>
		<autoFillFields>false</autoFillFields>
