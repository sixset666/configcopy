		<query>ВЫБРАТЬ
	СУММА(РасходныйКассовыйОрдер.СуммаДокумента) КАК Сумма,
	КОЛИЧЕСТВО(*) КАК Количество,
	РасходныйКассовыйОрдер.ДоговорВзаиморасчетов КАК Договор,
	РасходныйКассовыйОрдер.Контрагент КАК Банк,
	РасходныйКассовыйОрдер.ПодразделениеКомпании КАК Подразделение,
	РасходныйКассовыйОрдер.Организация КАК Организация,
	НАЧАЛОПЕРИОДА(РасходныйКассовыйОрдер.Дата, ДЕНЬ) КАК ДатаДня,
	МАКСИМУМ(НАЧАЛОПЕРИОДА(РасходныйКассовыйОрдер.Дата, ДЕНЬ)) КАК Дата,
	КОЛИЧЕСТВО(РасходныйКассовыйОрдер.Ссылка) КАК КоличествоРКО,
	0 КАК КоличествоБВ,
	МАКСИМУМ(РасходныйКассовыйОрдер.Ссылка) КАК РКО
ПОМЕСТИТЬ ВТ_СчетчикиРКО
ИЗ
	Документ.РасходныйКассовыйОрдер КАК РасходныйКассовыйОрдер
ГДЕ
	РасходныйКассовыйОрдер.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И РасходныйКассовыйОрдер.Организация = &amp;Организация
	И РасходныйКассовыйОрдер.СтатьяДДС = &amp;ВидОперацииРКО
	И РасходныйКассовыйОрдер.ПометкаУдаления = ЛОЖЬ
	И РасходныйКассовыйОрдер.Контрагент = &amp;Банк

СГРУППИРОВАТЬ ПО
	РасходныйКассовыйОрдер.ДоговорВзаиморасчетов,
	РасходныйКассовыйОрдер.Контрагент,
	РасходныйКассовыйОрдер.ПодразделениеКомпании,
	РасходныйКассовыйОрдер.Организация,
	НАЧАЛОПЕРИОДА(РасходныйКассовыйОрдер.Дата, ДЕНЬ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(ПоступлениеНаРасчетныйСчет.СуммаДокументаПриход) КАК Сумма,
	КОЛИЧЕСТВО(*) КАК Количество,
	ПоступлениеНаРасчетныйСчет.ДоговорВзаиморасчетов КАК Договор,
	ПоступлениеНаРасчетныйСчет.Контрагент КАК Банк,
	ПоступлениеНаРасчетныйСчет.ПодразделениеКомпании КАК Подразделение,
	ПоступлениеНаРасчетныйСчет.Организация КАК Организация,
	НАЧАЛОПЕРИОДА(ПоступлениеНаРасчетныйСчет.Дата, ДЕНЬ) КАК ДатаДня,
	МАКСИМУМ(НАЧАЛОПЕРИОДА(ПоступлениеНаРасчетныйСчет.Дата, ДЕНЬ)) КАК Дата,
	0 КАК КоличествоРКО,
	КОЛИЧЕСТВО(ПоступлениеНаРасчетныйСчет.Ссылка) КАК КоличествоБВ,
	МАКСИМУМ(ПоступлениеНаРасчетныйСчет.Ссылка) КАК ПоступленияНаРС
ПОМЕСТИТЬ ВТ_СчетчикиБВ
ИЗ
	Документ.Выписка КАК ПоступлениеНаРасчетныйСчет
ГДЕ
	ПоступлениеНаРасчетныйСчет.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ПоступлениеНаРасчетныйСчет.Организация = &amp;Организация
	И ПоступлениеНаРасчетныйСчет.СтатьяДДС = &amp;ВидОперацииБВ
	И ПоступлениеНаРасчетныйСчет.ПометкаУдаления = ЛОЖЬ
	И ПоступлениеНаРасчетныйСчет.Контрагент = &amp;Банк

СГРУППИРОВАТЬ ПО
	ПоступлениеНаРасчетныйСчет.ДоговорВзаиморасчетов,
	ПоступлениеНаРасчетныйСчет.Контрагент,
	ПоступлениеНаРасчетныйСчет.ПодразделениеКомпании,
	ПоступлениеНаРасчетныйСчет.Организация,
	НАЧАЛОПЕРИОДА(ПоступлениеНаРасчетныйСчет.Дата, ДЕНЬ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СУММА(ЕСТЬNULL(ВТ_Р.Количество, 0) + ЕСТЬNULL(ВТ_Б.Количество, 0)) КАК Количество,
	ЕСТЬNULL(ВТ_Р.Сумма, ВТ_Б.Сумма) КАК Сумма,
	ЕСТЬNULL(ВТ_Р.Договор, ВТ_Б.Договор) КАК Договор,
	ЕСТЬNULL(ВТ_Р.Банк, ВТ_Б.Банк) КАК Банк,
	ЕСТЬNULL(ВТ_Р.Подразделение, ВТ_Б.Подразделение) КАК Подразделение,
	ЕСТЬNULL(ВТ_Р.Организация, ВТ_Б.Организация) КАК Организация,
	ЕСТЬNULL(ВТ_Р.ДатаДня, ВТ_Б.ДатаДня) КАК ДатаДня,
	СУММА(ЕСТЬNULL(ВТ_Р.КоличествоРКО, 0)) КАК ОбщееКоличествоРКО,
	СУММА(ЕСТЬNULL(ВТ_Б.КоличествоБВ, 0)) КАК ОбщееКоличествоБВ
ПОМЕСТИТЬ ВТ_ФинальныеСчетчики
ИЗ
	ВТ_СчетчикиРКО КАК ВТ_Р
		ПОЛНОЕ СОЕДИНЕНИЕ ВТ_СчетчикиБВ КАК ВТ_Б
		ПО ВТ_Р.Сумма = ВТ_Б.Сумма
			И ВТ_Р.Банк = ВТ_Б.Банк
			И ВТ_Р.Организация = ВТ_Б.Организация
			И ВТ_Р.ДатаДня = ВТ_Б.ДатаДня
			И (ВТ_Р.Подразделение = ВТ_Б.Подразделение
				ИЛИ ВТ_Р.Подразделение.Родитель = ВТ_Б.Подразделение
				ИЛИ ВТ_Б.Подразделение.Родитель = ВТ_Р.Подразделение)

СГРУППИРОВАТЬ ПО
	ЕСТЬNULL(ВТ_Р.Сумма, ВТ_Б.Сумма),
	ЕСТЬNULL(ВТ_Р.Договор, ВТ_Б.Договор),
	ЕСТЬNULL(ВТ_Р.Банк, ВТ_Б.Банк),
	ЕСТЬNULL(ВТ_Р.Подразделение, ВТ_Б.Подразделение),
	ЕСТЬNULL(ВТ_Р.Организация, ВТ_Б.Организация),
	ЕСТЬNULL(ВТ_Р.ДатаДня, ВТ_Б.ДатаДня)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЕСТЬNULL(ВТ_Б.Договор, ВТ_Р.Договор) КАК ДоговорИнкассации,
	ЕСТЬNULL(ВТ_Б.Дата, ВТ_Р.Дата) КАК Дата,
	ВТ_Р.Сумма КАК СуммаРКО,
	ВТ_Б.Сумма КАК СуммаБВ,
	ВТ_Р.Количество КАК КоличествоРКО,
	ВТ_Б.Количество КАК КоличествоБВ,
	ВТ_Р.РКО КАК РКО,
	ВТ_Б.ПоступленияНаРС КАК ПоступленияНаРС,
	"СОПОСТАВЛЕНО" КАК Статус,
	ЕСТЬNULL(ВТ_Б.Банк, ВТ_Р.Банк) КАК Контрагент,
	ЕСТЬNULL(ВТ_Б.Организация, ВТ_Р.Организация) КАК Организация,
	ЕСТЬNULL(ВТ_Р.Сумма, 0) - ЕСТЬNULL(ВТ_Б.Сумма, 0) КАК Разница,
	1 КАК ТипСтроки,
	0 КАК ПоложительнаяРазница
ИЗ
	ВТ_СчетчикиРКО КАК ВТ_Р
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТ_СчетчикиБВ КАК ВТ_Б
		ПО ВТ_Р.Сумма = ВТ_Б.Сумма
			И ВТ_Р.ДатаДня = ВТ_Б.ДатаДня
			И ВТ_Р.Банк = ВТ_Б.Банк
			И ВТ_Р.Организация = ВТ_Б.Организация
ГДЕ
	ВТ_Р.Количество = ВТ_Б.Количество
	И ВТ_Р.Количество &gt; 0

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	РасходныйКассовыйОрдер.ДоговорВзаиморасчетов,
	НАЧАЛОПЕРИОДА(РасходныйКассовыйОрдер.Дата, ДЕНЬ),
	РасходныйКассовыйОрдер.СуммаДокумента,
	0,
	1,
	0,
	РасходныйКассовыйОрдер.Ссылка,
	NULL,
	"ЛИШНЕЕ РКО",
	РасходныйКассовыйОрдер.Контрагент,
	РасходныйКассовыйОрдер.Организация,
	РасходныйКассовыйОрдер.СуммаДокумента,
	2,
	0
ИЗ
	Документ.РасходныйКассовыйОрдер КАК РасходныйКассовыйОрдер
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ФинальныеСчетчики КАК ВТ_ФС
		ПО РасходныйКассовыйОрдер.СуммаДокумента = ВТ_ФС.Сумма
			И РасходныйКассовыйОрдер.ДоговорВзаиморасчетов = ВТ_ФС.Договор
			И РасходныйКассовыйОрдер.Контрагент = ВТ_ФС.Банк
			И РасходныйКассовыйОрдер.Организация = ВТ_ФС.Организация
			И (НАЧАЛОПЕРИОДА(РасходныйКассовыйОрдер.Дата, ДЕНЬ) = ВТ_ФС.ДатаДня)
ГДЕ
	(ВТ_ФС.ОбщееКоличествоРКО &lt;&gt; ВТ_ФС.ОбщееКоличествоБВ
			ИЛИ ВТ_ФС.ОбщееКоличествоБВ = 0)
	И РасходныйКассовыйОрдер.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И РасходныйКассовыйОрдер.Организация = &amp;Организация
	И РасходныйКассовыйОрдер.СтатьяДДС = &amp;ВидОперацииРКО
	И РасходныйКассовыйОрдер.ПометкаУдаления = ЛОЖЬ

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ПоступлениеНаРасчетныйСчет.ДоговорВзаиморасчетов,
	НАЧАЛОПЕРИОДА(ПоступлениеНаРасчетныйСчет.Дата, ДЕНЬ),
	0,
	ПоступлениеНаРасчетныйСчет.СуммаДокументаПриход,
	0,
	1,
	NULL,
	ПоступлениеНаРасчетныйСчет.Ссылка,
	"ЛИШНЕЕ ПОСТУПЛЕНИЕ",
	ПоступлениеНаРасчетныйСчет.Контрагент,
	ПоступлениеНаРасчетныйСчет.Организация,
	-ПоступлениеНаРасчетныйСчет.СуммаДокументаПриход,
	2,
	0
ИЗ
	Документ.Выписка КАК ПоступлениеНаРасчетныйСчет
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ФинальныеСчетчики КАК ВТ_ФС
		ПО ПоступлениеНаРасчетныйСчет.СуммаДокументаПриход = ВТ_ФС.Сумма
			И ПоступлениеНаРасчетныйСчет.ДоговорВзаиморасчетов = ВТ_ФС.Договор
			И ПоступлениеНаРасчетныйСчет.Контрагент = ВТ_ФС.Банк
			И ПоступлениеНаРасчетныйСчет.Организация = ВТ_ФС.Организация
			И (НАЧАЛОПЕРИОДА(ПоступлениеНаРасчетныйСчет.Дата, ДЕНЬ) = ВТ_ФС.ДатаДня)
ГДЕ
	(ВТ_ФС.ОбщееКоличествоРКО &lt;&gt; ВТ_ФС.ОбщееКоличествоБВ
			ИЛИ ВТ_ФС.ОбщееКоличествоРКО = 0)
	И ПоступлениеНаРасчетныйСчет.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
	И ПоступлениеНаРасчетныйСчет.Организация = &amp;Организация
	И ПоступлениеНаРасчетныйСчет.СтатьяДДС = &amp;ВидОперацииБВ
	И ПоступлениеНаРасчетныйСчет.ПометкаУдаления = ЛОЖЬ

УПОРЯДОЧИТЬ ПО
	ТипСтроки,
	Статус УБЫВ,
	ДоговорИнкассации,
	Дата,
	СуммаРКО,
	СуммаБВ</query>
	</dataSet>
	<calculatedField>
		<dataPath>Подразделение</dataPath>
		<expression>ВЫБОР

    КОГДА Строка(РКО.ПодразделениеКомпании) &lt;&gt; "" И Строка(ПоступленияНаРС.ПодразделениеКомпании) &lt;&gt; ""
        ТОГДА Строка(РКО.ПодразделениеКомпании) + " | " + Строка(ПоступленияНаРС.ПодразделениеКомпании)


    КОГДА Строка(РКО.ПодразделениеКомпании) = ""
        ТОГДА Строка(ПоступленияНаРС.ПодразделениеКомпании)

    ИНАЧЕ Строка(РКО.ПодразделениеКомпании)
КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Подразделение</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<totalField>
		<dataPath>Разница</dataPath>
		<expression>СУММА(ЕСТЬNULL(СуммаРКО, 0)) - СУММА(ЕСТЬNULL(СуммаБВ, 0))</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаБВ</dataPath>
		<expression>Сумма(СуммаБВ)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаРКО</dataPath>
		<expression>Сумма(СуммаРКО)</expression>
	</totalField>
	<parameter>
		<name>Организация</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Организация</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type xmlns:d4p1="http://v8.1c.ru/8.1/data/enterprise/current-config">d4p1:CatalogRef.Организации</v8:Type>
