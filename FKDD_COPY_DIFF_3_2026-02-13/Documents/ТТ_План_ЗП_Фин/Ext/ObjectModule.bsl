
Процедура ОбработкаПроведения(Отказ, РежимПроведения)
	
	НаборЗаписей = Движения.ТТ_ПланыЗП_Фин;
	
	Для каждого Стр из Планы Цикл     //АВ КВ
		
		НоваяЗапись = НаборЗаписей.Добавить();
		НоваяЗапись.Период=Дата;
		НоваяЗапись.Регистратор=ЭтотОбъект;
		НоваяЗапись.Сотрудник=Стр.Сотрудник;
		НоваяЗапись.Бренд=Стр.Бренд;
		НоваяЗапись.Параметр=Перечисления.ТТ_Параметры_ЗП_Фин.АВКВ;
		НоваяЗапись.Организация=Стр.Организация;
		НоваяЗапись.Значение=Стр.Значение;
		НоваяЗапись.Премия=Стр.Премия;
	КонецЦикла;
	
	Для каждого Стр из ПланыОСАГО Цикл     //ОСАГО шт
		
		НоваяЗапись = НаборЗаписей.Добавить();
		НоваяЗапись.Период=Дата;
		НоваяЗапись.Регистратор=ЭтотОбъект;
		НоваяЗапись.Сотрудник=Стр.Сотрудник;
		НоваяЗапись.Параметр=Перечисления.ТТ_Параметры_ЗП_Фин.ОСАГО;
		НоваяЗапись.Значение=Стр.Значение;
		НоваяЗапись.Премия=Стр.Премия;
	КонецЦикла;

	
	Для каждого Стр из ПланыСтраховки Цикл
		
		НоваяЗапись = НаборЗаписей.Добавить();
		НоваяЗапись.Период=Дата;
		НоваяЗапись.Регистратор=ЭтотОбъект;
		НоваяЗапись.Сотрудник=Стр.Сотрудник;
		НоваяЗапись.Параметр=Стр.Параметр;
		
		НоваяЗапись.ПрограммаСтрахования=Стр.ПрограммаСтрахования;
		НоваяЗапись.СтраховаяКомпания=Стр.СтраховаяКомпания;
		НоваяЗапись.ВидСтрахования=Стр.ВидСтрахования;

		НоваяЗапись.Значение=Стр.Значение;
		НоваяЗапись.Премия=Стр.СуммаПремии;
	КонецЦикла;
	
	Для каждого Стр из ПланыБанки Цикл
		
		НоваяЗапись = НаборЗаписей.Добавить();
		НоваяЗапись.Период=Дата;
		НоваяЗапись.Регистратор=ЭтотОбъект;
		НоваяЗапись.Сотрудник=Стр.Сотрудник;
		НоваяЗапись.Параметр=Стр.Параметр;
		
		НоваяЗапись.Банк=Стр.Банк;

		НоваяЗапись.Значение=Стр.Значение;
		НоваяЗапись.Премия=Стр.СуммаВыдачи;
	КонецЦикла;
	
	
	Для каждого Стр из ПланыПролонгация Цикл
		
		НоваяЗапись = НаборЗаписей.Добавить();
		НоваяЗапись.Период=Дата;
		НоваяЗапись.Регистратор=ЭтотОбъект;
		НоваяЗапись.Сотрудник=Стр.Сотрудник;
		НоваяЗапись.Параметр=Перечисления.ТТ_Параметры_ЗП_Фин.Пролонгация;
		НоваяЗапись.Значение=Стр.Значение;
	КонецЦикла;

	НаборЗаписей.Записать();

	
КонецПроцедуры


