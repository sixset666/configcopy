	(&amp;ОбъектДоступа = ""
			ИЛИ ВариантыОтчетов.ОбъектДоступа = &amp;ОбъектДоступа) И 
	(&amp;ОтборНаименование = ""
			ИЛИ ВариантыОтчетов.Наименование ПОДОБНО "%" + &amp;ОтборНаименование + "%") И 
	(&amp;ОтборАвтор = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
			ИЛИ ВариантыОтчетов.Автор = &amp;ОтборАвтор)</QueryText>
